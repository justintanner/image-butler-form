  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justintanner/image-butler-form/issues"
  },
  "homepage": "https://github.com/justintanner/image-butler-form#readme",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "nyc": "^11.2.1",
    "prettier": "^1.5.3"
  },
  "dependencies": {
    "handlebars": "^4.0.11",
    "s3-browser-direct-upload": "^0.1.2",
